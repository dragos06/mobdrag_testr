generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Device {
  id Int @id @default(autoincrement())
  model String
  os String
  screenResolution String
  status Int
  usedBy String?
  notes String?
  lastModified DateTime @default(now())
}